 ;; Keywords:
 ;; Compatibility: Emacs 23+

 ;;; Installation:

 ;;
 ;;    (add-hook 'ruby-mode-hook
 ;;          (lambda ()
 ;;            (require 'autopair-padding)
 ;;            (setq autopair-dont-pad '(:comment :string))
 ;;            (setq autopair-handle-action-fns
 ;;                  (list #'autopair-default-handle-action
